{
  "manifest_version": 3,
  "name": "Custom Search Home Page",
  "version": "1.0.0",
  "description": "A beautiful custom home page with Google search and shortcuts",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "host_permissions": [
    "https://corsproxy.io/*",
    "https://suggestqueries.google.com/*"
  ]
}
